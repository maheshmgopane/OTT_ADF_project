{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ott-datafactory-001-dev"
		},
		"LS_ADLSgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSgen2'"
		},
		"LS_Onprem_File_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Onprem_File'"
		},
		"LS_ADLSgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ottadlsgen2dev001.dfs.core.windows.net/"
		},
		"LS_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://OTT-key-vault-adf.vault.azure.net/"
		},
		"LS_Synapse_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://ott-synapse-dev-001.dev.azuresynapse.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ott-datafactory-001-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSgen2_accountKey": {
						"value": "[parameters('LS_ADLSgen2_accountKey')]"
					},
					"LS_Onprem_File_connectionString": {
						"value": "[parameters('LS_Onprem_File_connectionString')]"
					},
					"LS_ADLSgen2_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSgen2_properties_typeProperties_url')]"
					},
					"LS_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_Synapse_properties_typeProperties_endpoint": {
						"value": "[parameters('LS_Synapse_properties_typeProperties_endpoint')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}